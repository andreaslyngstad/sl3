<% if @logs.nil? %>
	<%= form_for(current_firm.logs.new, :url => add_log_timesheet_path ) do |l| %>
	<%= l.hidden_field :user_id, :value => current_user.id %>
	<%= render :partial => "logs/log_menu_form", :locals => {:l => l } %>
	<% end %>
<% else %>
	<%= form_for(current_firm.logs.new, :remote => true) do |l| %>  
	<%= render :partial => "logs/log_menu_form", :locals => {:l => l } %>
	<% end %>
<% end %>
